<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Contenido.Master.cs" Inherits="Vista.Site4" %>

<!DOCTYPE html>

<html>
<head runat="server">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <!-- font awesome -->
    <script src="https://kit.fontawesome.com/7769f8ac48.js" crossorigin="anonymous"></script>
    <link
        href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
        rel="stylesheet">
    <!-- css template -->
    <link href="css/sb-admin-2.min.css" rel="stylesheet">
    <!-- css bootstrap 
    <link href="vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">-->
    <style>
        * {
            font-family: Tahoma, Verdana, sans-serif;
        }
    </style>
    <%
        if (Session["idRol"] == null)
            off();
    %>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
</head>
<body id="page-top">

    <div id="wrapper">

        <!-- Sidebar -->
        <ul class="navbar-nav bg-secondary sidebar sidebar-dark accordion" id="accordionSidebar">

            <br />
            <br />
            <br />
            <!-- Sidebar - Brand -->
            <a class="sidebar-brand d-flex align-items-center justify-content-center" href="dashboard.aspx">
                <div class="sidebar-brand-icon">
                </div>
                <div class="sidebar-brand-text mx-2">Red Vallecaucana de Salud</div>
            </a>

            <br />
            <br />
            <br />
            <hr class="sidebar-divider">
            <li class="nav-item">
                <a class="nav-link text-center" href="dashboard.aspx">
                    <span>Inicio</span></a>
            </li>


            <!-- Nav Items -->
            <%
                if (int.Parse(Session["idRol"].ToString()) == 1)
                {
            %>
            <!-- Menu para admin -->
            <li class="nav-item">
                <a class="nav-link text-center" href="agregarEnte.aspx">
                    <span>Crear ente</span></a>
            </li>

            <li class="nav-item">
                <a class="nav-link text-center" href="agregarEspecialista.aspx">
                    <span>Crear especialista</span></a>
            </li>

            <li class="nav-item">
                <a class="nav-link text-center" href="consultarCitas.aspx">
                    <span>Consultar citas</span></a>
            </li>

            <li class="nav-item">
                <a class="nav-link text-center" href="especialidades.aspx">
                    <span>Consultar especialista</span></a>
            </li>

            <li class="nav-item">
                <a class="nav-link text-center" href="enciclopedia.aspx">
                    <span>Consultar enfermedades</span></a>
            </li>

            <%
                }
            %>

            <%
                if (int.Parse(Session["idRol"].ToString()) == 2)
                {
            %>
            <!-- Menu para contenido -->

            <li class="nav-item">
                <a class="nav-link text-center" href="agregarEspecialista.aspx">
                    <span>Crear especialista</span></a>
            </li>

            <li class="nav-item">
                <a class="nav-link text-center" href="especialidades.aspx">
                    <span>Consultar especialista</span></a>
            </li>

            <%
                }
            %>
            <%
                if (int.Parse(Session["idRol"].ToString()) == 3)
                {
            %>
            <!-- Menu normal  -->

            <li class="nav-item">
                <a class="nav-link text-center" href="consultarCitas.aspx">
                    <span>Calificar citas</span></a>
            </li>

            <li class="nav-item">
                <a class="nav-link text-center" href="especialidades.aspx">
                    <span>Consultar especialista</span></a>
            </li>

            <li class="nav-item">
                <a class="nav-link text-center" href="enciclopedia.aspx">
                    <span>Consultar enfermedades</span></a>
            </li>

            <%
                }
            %>

            <hr class="sidebar-divider d-none d-md-block">

        </ul>
        <!-- End of Sidebar -->

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">
            <form role="form" method="post" runat="server">

                <!-- Main Content -->
                <div id="content">

                    <!-- Topbar -->
                    <nav class="navbar navbar-expand navbar-light bg-dark topbar mb-4 static-top ">



                        <!-- Topbar Navbar -->
                        <ul class="navbar-nav ml-auto">

                            <!-- Nav Item - User Information -->
                            <li class="nav-item dropdown no-arrow">
                                <a class="nav-link dropdown-toggle" href="#" id="userDropdown2" role="button"
                                    data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <span class="mr-2 d-none d-lg-inline text-gray-600 small">BIENVENIDO: 
                                        <% Response.Write(Session["nombre"]);%>&nbsp;<%Response.Write(Session["apellido"]);%>
                                    </span>
                                </a>
                                <!-- Dropdown - User Information -->
                                <div class="dropdown-menu dropdown-menu-right shadow animated--grow-in"
                                    aria-labelledby="userDropdown">
                                    <center>
                                        <a class="btn m-0 p-0" href="login.aspx">SALIR</a>
                                    </center>
                                </div>
                            </li>
                        </ul>
                    </nav>
                    <!-- End of Topbar -->

                    <!-- Contenido principal -->
                    <div class="container-fluid">
                        <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
                        </asp:ContentPlaceHolder>
                    </div>

                </div>

            </form>
        </div>
        <!-- End of Content Wrapper -->
    </div>

    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>
    <script src="https://code.jquery.com/jquery-3.5.1.js"></script>
    <script src="js/popper.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <asp:ContentPlaceHolder ID="footer" runat="server">
    </asp:ContentPlaceHolder>
    <script src="js/sb-admin-2.min.js"></script>
</body>
</html>

